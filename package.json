{
  "name": "brainsatplay-components",
  "version": "0.0.0",
  "author": "Garrett Flynn <garrettmflynn@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "concurrently \"yarn:storybook\" \"yarn:tailwind\" -k --kill-others-on-fail -p \"{name}|\" -n \"SB,TW\" -c \"magenta,cyan\"",
    "tailwind": "tailwindcss -i src/styles/tailwind.css -o src/index.css --watch",
    "tailwind:build": "tailwindcss -i src/styles/tailwind.css -o src/index.css",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "yarn tailwind:build && build-storybook --docs -c .storybook -o docs -s src/assets",
    "lint": "eslint ./src --ext .ts,.tsx",
    "build:npm": "tsc && node scripts copy:styles",
    "pub": "yarn run lint && node scripts clean:dist && yarn build:npm && npm publish",
    "build-storybook": "build-storybook"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "resolutions": {
    "mini-css-extract-plugin": "2.4.5"
  },
  "dependencies": {
    "@giveback007/util-lib": "^0.23.2",
    "classnames": "^2.3.1",
    "sb": "^6.4.18",
    "tslib": "~2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-storysource": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/web-components": "^6.4.18",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "fs-extra": "^10.0.0",
    "image-webpack-loader": "^8.1.0",
    "lit": "^2.1.2",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "tailwind-scrollbar": "^1.3.1",
    "typescript": "^4.4.2",
    "webpack": "^5.66.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
